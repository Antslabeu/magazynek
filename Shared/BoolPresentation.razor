<style>
    .fa-circle-xmark {
        color: var(--accent-error);
    }
    .fa-circle-check {
        color: var(--accent-success);
    }
</style>


@if(Value)
{
    <i class="fa-solid fa-circle-check"></i>
}
else 
{
    <i class="fa-solid fa-circle-xmark"></i>
}


@code {
    [Parameter] public bool Value { get; set; }
}